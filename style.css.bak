}

small, #sidebar ul ul li, #sidebar ul ol li, .nocomments, .postmetadata, blockquote, strike {
	color: #777;
	}

code {
	margin-bottom: 10px;
}

acronym, abbr, span.caps {
	font-size: 0.9em;
	letter-spacing: .07em;
}


/* Begin Structure */

.view_options {
	position: absolute;
	top: 51px;
	margin-left: 640px;
}
.view_options_label {
	float: left;
	width: 100px;
	margin: 4px 0px 0 0;
	padding-top: 9px;
}

.view_options ul {
	list-style: none;
	float: left;
}
.view_options ul li {
	float: left;
	margin-right: 16px;
}
.view_options ul li.view_options_3 {
	margin-right: 0;
}
.view_options ul li a {
	display: block;
	width: 38px;
	padding: 9px;
	height: 29px;
	overflow: hidden;
	text-indent: -9999px;
	
	border-radius:3px;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
}
.view_options ul li.view_options_1 a { background: url(images/view_1_out.png) no-repeat 9px 9px; }
.view_options ul li.view_options_1 a.on, .view_options ul li.view_options_1 a.on:hover, .view_options ul li.view_options_1 a:active { background: #27393A url(images/view_1_on.png) no-repeat 9px 9px; }
.view_options ul li.view_options_1 a:hover { background: #27393A url(images/view_1_out.png) no-repeat 9px 9px; }

.view_options ul li.view_options_2 a { background: url(images/view_2_out.png) no-repeat 9px 9px; }
.view_options ul li.view_options_2 a.on, .view_options ul li.view_options_2 a.on:hover, .view_options ul li.view_options_2 a:active { background: #27393A url(images/view_2_on.png) no-repeat 9px 9px; }
.view_options ul li.view_options_2 a:hover { background: #27393A url(images/view_2_out.png) no-repeat 9px 9px; }

.view_options ul li.view_options_3 a { background: url(images/view_3_out.png) no-repeat 9px 9px; }
.view_options ul li.view_options_3 a.on, .view_options ul li.view_options_3 a.on:hover, .view_options ul li.view_options_3 a:active { background: #27393A url(images/view_3_on.png) no-repeat 9px 9px; }
.view_options ul li.view_options_3 a:hover { background: #27393A url(images/view_3_out.png) no-repeat 9px 9px; }

.view_tags {
	position: absolute;
