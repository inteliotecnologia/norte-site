html {
	overflow: -moz-scrollbars-vertical;
	margin: 0;
	padding: 0;	
}

* {
	margin: 0;
	padding: 0;	
	outline: 0;
}

body {
	font: normal 14px 'Droid Serif', arial, serif; 
	font-weight: 100;
	background: #000 url('images/body-bg.png') repeat-x;
	color: #999;
	padding: 0;
	line-height: 22px;
}
body.home {
	background: #000 url('images/body-home-bg.png') repeat-x;
}
body.inner {
	background: #000 url('images/body-inner-bg.png') repeat-x; 
}

body.inner-2 #content {
	padding-bottom: 50px;
}

a:link, a:active, a:hover, a:visited {
	color: #fff;
	text-decoration: none;
}
a:hover {
	text-decoration: none;
}

br.clear {
	clear: both;
}

h1, h2, h3, h4 {
	font-weight: 100;
}

h2 {
	font-size: 26px;
}

h2.pagetitle {
	font: normal 36px 'Droid Serif', arial, serif; 
	color: #DF7F97;
	margin-bottom: 79px;
	letter-spacing: -0.5px;
}
#content .pagetitle-top {
	margin-bottom: 38px;
}
#content .pagetitle-top a {
	color: #fff;
	display: block;
	height: 25px;
	width: 25px;
	text-indent: -9999px;
	overflow: hidden;
	margin-top: 4px;
	margin-bottom: 42px;
	background: url('images/volta.png') no-repeat;
}
#content .pagetitle-top a:hover {
	background: url('images/volta_h.png') no-repeat;
}

h2.sectiontitle {
	font: normal 30px 'Droid Serif', arial, serif; 
	font-weight: 100;
	color: #DF7F97;
	margin-bottom: 20px;
}
h2.posttitle {
	color: #22B2BA;
}

h3 {
	color: #22B2BB;
	font-weight: 100;
}

#out {
	position: relative;
	margin: 0 auto;
	padding: 0 10px;
	width: 950px;
}
#page {
	position: relative;
	margin: 0 auto;
	padding: 0;
	width: 940px;
}

#header {
	position: relative;
	height: 150px;
}

#logo {
	position: absolute;
	width: 150px;
	height: 150px;
	left: 0;
	top: 0;
	background: url('images/logo_topo.gif') no-repeat;
}
#logo h1 a {
	display: block;
	width: 150px;
	height: 150px;
	text-indent: -9999px;
	overflow: hidden;
}

#busca-area {
	position: absolute;
	top: 53px;
	right: 5px;	
	background: url(images/lupa.png) no-repeat 182px 10px;
}
#busca-area.buscando {
	background: url(images/loading_busca.gif) no-repeat 182px 10px;
}

#busca-area input {
	width: 170px;
	border: 0;
	border-radius:4px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	background: url(images/branco20-bg.png);
	
	font: normal 14px 'Droid Serif', arial, serif; 
	
	box-shadow:inset 1px 1px 2px #387579;
    -moz-box-shadow:inset 1px 1px 2px #387579;
    -webkit-box-shadow:inset 1px 1px 2px #387579;

	
	color: #fff;
	padding: 10px 30px 9px 10px;
}
#busca-area input:focus {
	background: url(images/branco30-bg.png);
}

#sugestoes {
	position: absolute;
	width: 210px;
	display:none;
	margin-top: 5px;
	z-index: 100;
}



#resultado {
	width:210px;
	font-size:10px;
	line-height:14px;
	list-style: none;
}
#resultado li {
	
	
}
#resultado a {
	display:block;
	border-bottom: 1px solid #dfdfdf;
	clear:left;
	height:32px;
	text-decoration:none;
	color: #000;
	background: #fff;
	
	/*border-right: 1px solid #000;
	border-left: 1px solid #000;*/
}

#resultado li.ultima a {
	/*border-bottom: 1px solid #000;*/
}


#resultado a:hover, #resultado a.itemhover {
	background:#eaeaea;
	color:#000;
}
#resultado a img {
	float:left;
	margin-right: 6px;
	padding: 3px 0 3px 3px;
}
#resultado a span.resultado-titulo {
	display:block;
	padding-top:8px;
}

#resultado li.category, #resultado li.separator {
	font-size:10px;
	padding: 8px;
	display:block;
	background: #000;
	color: #fff;
	
	border-top-left-radius:5px;
	-moz-border-radius-topleft:5px;
	-webkit-border-top-left-radius:5px;
	
	border-top-right-radius:5px;
	-moz-border-radius-topright:5px;
	-webkit-border-top-right-radius:5px;
}
#resultado li.ultima a, #resultado li.ultima img {
	border-bottom-left-radius:5px;
	-moz-border-radius-bottomleft:5px;
	-webkit-border-bottom-left-radius:5px;
	
	border-bottom-right-radius:5px;
	-moz-border-radius-bottomright:5px;
	-webkit-border-bottom-right-radius:5px;
}

#resultado li.separator {
	border-radius:5px;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
}



#menu {
	position: absolute;
	top: 60px;
	right: 226px;
}
#menu_controls {
	display: none;
}

#menu ul {
	list-style: none;
}
#menu ul li {
	float: left;
	margin-left: 10.5px;
}
#menu ul li a {
	border-radius:4px;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	
	font: normal 14px 'Droid Serif', arial, serif; 
	padding: 10px 12px 9px 12px;
}
#menu ul li a.current {
	background: url(images/branco30-bg.png);
	
	box-shadow:inset 1px 1px 2px #387579;
    -moz-box-shadow:inset 1px 1px 2px #387579;
    -webkit-box-shadow:inset 1px 1px 2px #387579;
}
#menu ul li a:hover {
	background: url(images/branco30-bg.png);
	text-decoration: none;
}
#menu ul li a.current:hover {
	background: url(images/branco45-bg.png);
}
#menu ul li a:active {
	background: url(images/branco30-bg.png);
	
	box-shadow:inset 1px 1px 2px #387579;
    -moz-box-shadow:inset 1px 1px 2px #387579;
    -webkit-box-shadow:inset 1px 1px 2px #387579;
}
#content {
	position: relative;
	width: 940px;
	padding: 51px 0 20px 0;
	float: left;
	background: url('images/dotted.gif') repeat-x bottom;
}
.inner #content {
	padding-top: 4px;
}
#content a {
	color: #21B1B9;
}
#content a.mais {
	display: block;
	text-indent: -999px;
	overflow: hidden;
	width: 25px;
	height: 25px;
	background: url('images/mais.png') no-repeat;
}
#content a.mais:hover {
	background: url('images/mais_h.png') no-repeat;
	color: #fff;
	text-decoration: none;
}

body.home #content { padding: 0; }

#footer {
	margin: 0;
	padding-top: 35px;
	width: 940px;
	font-size: 13px;
	float: left;
}
#footer-padding {
	position: relative;
	clear: both;
	height: 80px;
}
#footer.sem-margem {
	margin-top: 0;
}
#footer-infos {
	width: 610px;
	float: left;
	font-size: 13px;
}
#footer-networks, #post-networks {
	width: 330px;
	float: left;
}
#post-networks {
	width: auto;
}

.share {
	margin-bottom: 40px;
	clear: both;
}
.share1 {
	width: 120px;
	float: left;
}

#footer-networks ul, #post-networks ul {
	list-style: none;
	float: right;
}

#footer-networks ul li, #post-networks ul li {
	float: left;
	margin-left: 10px;
}
#post-networks ul li {
	margin-left: 0;	
	margin-right: 10px;
}
#footer-networks ul li a, #post-networks ul li a {
	display: block;
	text-indent: -999px;
	overflow: hidden;
	width: 25px;
	height: 25px;
}

ul li a.ico-rss { background: url('images/ico-rss.png') no-repeat; }
ul li a.ico-rss:hover { background: url('images/ico-rss_2.png') no-repeat; }

ul li a.ico-facebook { background: url('images/ico-facebook.png') no-repeat; }
ul li a.ico-facebook:hover { background: url('images/ico-facebook_2.png') no-repeat; }

